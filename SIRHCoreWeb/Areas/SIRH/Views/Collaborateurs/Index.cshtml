                <form action="/Home/InsertUser" class="kt-form kt-form--label-right">
                    <div class="kt-portlet__body">
                        <div class="form-group row form-group-marginless kt-margin-t-20">
                            <label class="col-lg-1 col-form-label">Civilité* : </label>
                            <div class="col-lg-3">
                                <SELECT class="form-control" name="civilite" id="civilite" size="1">
                                    <option selected>
                                    <OPTION>M.
                                    <OPTION>Mme
                                    <OPTION>Mlle
                                </SELECT>
                            </div>
                            <label class="col-lg-1 col-form-label">Nom:</label>
                            <div class="col-lg-3">
                                <input type="text" class="form-control" name="name" id="name" required placeholder="Nom">

                            </div>
                            <label class="col-lg-1 col-form-label">Prénom:</label>
                            <div class="col-lg-3">
                                <div class="input-group">
                                    <input type="text" class="form-control" name="prenom" id="prenom" required placeholder="Prénom">
                                </div>
                            </div>
                        </div>
                        <div class="kt-separator kt-separator--border-dashed kt-separator--space-lg kt-separator--portlet-fit"></div>
                        <div class="form-group row form-group-marginless">
                            <label class="col-lg-1 col-form-label">Matricule:</label>
                            <div class="col-lg-3">
                                <input type="number" class="form-control" name="matricule" id="matricule" required placeholder="Entrez votre matricule">
                            </div>
                            <label class="col-lg-1 col-form-label">Département:</label>
                            <div class="col-lg-3">
                                <div class="kt-input-icon">
                                    <input type="text" class="form-control" name="departement" id="departement" placeholder="Département">
                                </div>
                            </div>
                            <label class="col-lg-1 col-form-label">Téléphone :</label>
                            <div class="col-lg-3">
                                <div class="kt-input-icon">
                                    <input type="text" class="form-control" name="phone" id="phone" required placeholder="Numéro téléphone">
                                </div>
                            </div>
                        </div>
                        <div class="kt-separator kt-separator--border-dashed kt-separator--space-lg kt-separator--portlet-fit"></div>

                    </div>
                    <div class="kt-portlet__foot">
                        <div class="kt-form__actions">
                            <div class="row">
